{
  "name": "carecircleapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reverse:5173": "adb reverse tcp:5173 tcp:5173", 
    "expo:start": "expo start --localhost -c",
    "kill:mac": "kill -9 $(lsof -ti:8081) || true",
    "kill:win": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :8081') do taskkill /F /PID %a",
    "start:mac": "npm run kill:mac && EXPO_OFFLINE=1 npm run expo:start",
    "start:win": "npm run kill:win && set EXPO_OFFLINE=1&& npm run expo:start"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "expo": "~54.0.33",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.0.0",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
