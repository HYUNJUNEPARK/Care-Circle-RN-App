{
  "name": "carecircleapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "expo:start": "expo start --localhost -c",
    "kill:mac": "kill -9 $(lsof -ti:8081) || true",
    "kill:win": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :8081') do taskkill /F /PID %a",
    "reverse:port": "adb reverse tcp:5173 tcp:5173 && adb reverse tcp:4000 tcp:4000",
    "start:m": "npm run kill:mac && APP_ENV=development EXPO_OFFLINE=1 npm run expo:start",
    "start:m:prod": "npm run kill:mac && APP_ENV=production EXPO_OFFLINE=1 npm run expo:start",
    "start:w": "npm run kill:win && set APP_ENV=development&& set EXPO_OFFLINE=1&& npm run expo:start",
    "start:w:prod": "npm run kill:win && set APP_ENV=production&& set EXPO_OFFLINE=1&& npm run expo:start"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@react-navigation/stack": "^7.7.2",
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-checkbox": "^5.0.8",
    "expo-status-bar": "~3.0.9",
    "firebase": "^12.9.0",
    "install": "^0.13.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.0.0",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
